{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug DSA",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/main.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        { "name": "TERM", "value": "xterm-256color" }
      ],
      "externalConsole": true,
      "MIMode": "lldb",
      "preLaunchTask": "build all",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
